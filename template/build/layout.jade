include lib

!!! 5
include html
  head
    include head
  body
    #wrap.boxed
      header
        .container.clearfix
          .one-third.column
            .logo
              a(href='index.html')
                img(src='../assets/img/logo_light.png', alt='Testacular')
          .two-thirds.column
            nav#menu.navigation
              ul#nav
                mixin navigation(metadata.section)
                mixin versionDropdown(versions, currentVersion)
          .sixteen.columns
            hr

      block content
        .container.clearfix
          .sixteen.columns
            h1.page-title
              = pageTitle
              span.line
          .four.columns.bottom
            h2.title.bottom-2
              | Table of Content
              span.line
            mixin sidebar(toh)
          .page-columns
            #markdown.twelve.columns.bottom
              != content
      footer
        .container
          include footer

    // Placed at the end of the document so the pages load faster 
    include scripts
